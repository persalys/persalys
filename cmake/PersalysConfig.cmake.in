#                                               -*- cmake -*-
#
#  PersalysConfig.cmake(.in)
#
#  Find Persalys includes and libraries
#
#  Copyright 2015-2021 EDF-Phimeca
#

# Use the following variables to compile and link against Persalys:
#  PERSALYS_FOUND          - True if Persalys was found on your system
#  PERSALYS_USE_FILE       - The file making Persalys usable
#  PERSALYS_DEFINITIONS    - Definitions needed to build with Persalys
#  PERSALYS_INCLUDE_DIRS   - List of directories where Persalys' header file are
#  PERSALYS_LIBRARY        - Library name
#  PERSALYS_LIBRARIES      - List of libraries to link against
#  PERSALYS_LIBRARY_DIRS   - List of directories containing Persalys' libraries
#  PERSALYS_ROOT_DIR       - The base directory of Persalys
#  PERSALYS_VERSION_STRING - A human-readable string containing the version
#  PERSALYS_VERSION_MAJOR  - The major version of Persalys
#  PERSALYS_VERSION_MINOR  - The minor version of Persalys
#  PERSALYS_VERSION_PATCH  - The patch version of Persalys

set ( PERSALYS_FOUND 1 )
set ( PERSALYS_USE_FILE     "@PERSALYS_USE_FILE@" )

set ( PERSALYS_DEFINITIONS  "@PERSALYS_DEFINITIONS@" )
set ( PERSALYS_INCLUDE_DIR  "@PERSALYS_INCLUDE_DIR@" )
set ( PERSALYS_INCLUDE_DIRS "@PERSALYS_INCLUDE_DIRS@" )
set ( PERSALYS_LIBRARY      "@PERSALYS_LIBRARY@" )
set ( PERSALYS_LIBRARIES    "@PERSALYS_LIBRARIES@" )
set ( PERSALYS_LIBRARY_DIRS "@PERSALYS_LIBRARY_DIRS@" )
set ( PERSALYS_ROOT_DIR     "@PERSALYS_ROOT_DIR@" )

set ( PERSALYS_VERSION_STRING "@PERSALYS_VERSION_STRING@" )
set ( PERSALYS_VERSION_MAJOR  "@PERSALYS_VERSION_MAJOR@" )
set ( PERSALYS_VERSION_MINOR  "@PERSALYS_VERSION_MINOR@" )
set ( PERSALYS_VERSION_PATCH  "@PERSALYS_VERSION_PATCH@" )

set (PERSALYS_PYTHON@PYTHON_VERSION_MAJOR@_MODULE_PATH "@PERSALYS_PYTHON_MODULE_PATH@")
